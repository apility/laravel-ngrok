{
    "name": "apility/laravel-ngrok",
    "description": "Share Laravel application with ngrok.",
    "keywords": [
        "apility",
        "laravel-ngrok"
    ],
    "homepage": "https://github.com/apility/laravel-ngrok",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Correa",
            "email": "jn.j41r0@gmail.com"
        },
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/console": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/pagination": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "symfony/process": "^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.3|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Apility\\Laravel\\Ngrok\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apility\\Laravel\\Ngrok\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apility\\Laravel\\Ngrok\\NgrokServiceProvider"
            ]
        }
    }
}
